{
  "env": {
    "development": {
      "presets": [
        "next/babel",
        "@babel/typescript"
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@nextjs-fullstack/common": "../common/"
        }
      }
    ]
  ]
}